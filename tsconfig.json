{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@lib/*": ["src/lib/*"],
      "@assets/*": ["public/assets/*"],
      "@layouts/*": ["src/layouts/*"],
      "@api/*": ["src/api/*"],
      "@features/*": ["src/features/*"],
      "@components/*": ["src/components/*"],
      "@contexts/*": ["src/contexts/*"],
      "@views/*": ["src/views/*"],
      "@common-ui/*": ["src/components/ui/common/*"],
      "@dashboard-ui/*": ["src/views/dashboard/common/*"],
      "@dashboard-forms/*": ["src/components/forms/dashboard/*"],
      "@dashboard-views/*": ["src/views/dashboard/*"],
      "@store/*": ["src/store/*"],
      "@services/*": ["src/services/*"],
      "@root/*": ["*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pdf-worker.ts",
    "pdf-worker.js",
    "pages/register/facebooskSignIn tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}